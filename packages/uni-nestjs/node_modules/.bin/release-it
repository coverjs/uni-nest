#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hacxy/Projects/uni-nestjs/node_modules/.pnpm/release-it@17.1.1/node_modules/release-it/bin/node_modules:/Users/hacxy/Projects/uni-nestjs/node_modules/.pnpm/release-it@17.1.1/node_modules/release-it/node_modules:/Users/hacxy/Projects/uni-nestjs/node_modules/.pnpm/release-it@17.1.1/node_modules:/Users/hacxy/Projects/uni-nestjs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hacxy/Projects/uni-nestjs/node_modules/.pnpm/release-it@17.1.1/node_modules/release-it/bin/node_modules:/Users/hacxy/Projects/uni-nestjs/node_modules/.pnpm/release-it@17.1.1/node_modules/release-it/node_modules:/Users/hacxy/Projects/uni-nestjs/node_modules/.pnpm/release-it@17.1.1/node_modules:/Users/hacxy/Projects/uni-nestjs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/release-it@17.1.1/node_modules/release-it/bin/release-it.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/release-it@17.1.1/node_modules/release-it/bin/release-it.js" "$@"
fi
